# Example environment variables for contributors
# Copy this file to .env and update with your values

# Backend Configuration (use mock for local development)
VITE_BACKEND_URL=http://localhost:3000/

# Authentication (optional - will use mock auth if not provided)
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_key_here

# Google Maps API (optional - will use mock map if not provided)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# Firebase Configuration (optional - will use mock firebase if not provided)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Development Mode (set to true for mock data)
VITE_USE_MOCK_DATA=true
